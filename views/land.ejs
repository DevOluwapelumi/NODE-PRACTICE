h2 {
  margin: 0;
}

h2:after {
  content: "";
  display: block;
  width: 100%;
  height: 2px;
  background: #fff;
  border-radius: 2px;
  margin: 1em 0;
}

.glow {
  max-width: 1200px;
  height: 700px;
  margin: 50px auto;
  position: relative;
  border-radius: 10px;
  background: #ed04f5;
  padding: 30px;
}

.glow:after {
  position: absolute;
  width: 90%;
  height: 80%;
  top: 25%;
  left: 0;
  right: 0;
  margin: 0 auto;
  z-index: -1;
  content: "";
  filter: blur(50px);
  background: linear-gradient(90deg, rgba(131,58,180,1) 0%, rgba(253,29,29,1) 50%, rgba(252,176,69,1) 100%);
}
